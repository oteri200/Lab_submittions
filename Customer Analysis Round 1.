    "#Rearrange the columns in the dataframe as needed\n",
    "df_1=df1[[\"customer\",\"gender\",\"state\",\"customer lifetime value\",\"education\",\"income\",\"monthly premium auto\",\"number of open complaints\",\n",
    "                 \"policy type\",\"total claim amount\",\"vehicle class\"]]\n",
    "df_2=df2[[\"customer\",\"gender\",\"state\",\"customer lifetime value\",\"education\",\"income\",\"monthly premium auto\",\"number of open complaints\",\n",
    "                 \"policy type\",\"total claim amount\",\"vehicle class\"]]\n",
    "df_3=df3[[\"customer\",\"gender\",\"state\",\"customer lifetime value\",\"education\",\"income\",\"monthly premium auto\",\"number of open complaints\",\n",
    "                 \"policy type\",\"total claim amount\",\"vehicle class\"]]\n",
